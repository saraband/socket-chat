{
  "name": "socket-chat-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -n SERVER,CLIENT \"yarn server:dev\" \"yarn client:dev\" -c bgBlue.black,bgRed.black -t \"HH:mm:ss\" -k -p \"{time} - {name}\"",
    "server:dev": "nodemon --config server/nodemon.json --exec \"babel-node server/src/index.js\"",
    "server:build": "webpack --config server/webpack.server.prod.js",
    "server:run": "node server/dist/server.js",
    "client:dev": "webpack-dev-server --config client/webpack.client.dev.js",
    "client:build": "webpack --config client/webpack.client.prod.js"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "express": "^4.16.4",
    "helmet": "^3.15.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
